<odoo>
    <data>

        <record id="account_sync_connector_view_form" model="ir.ui.view">
            <field name="name">account.sync.connector.form</field>
            <field name="model">account.sync.connector</field>
            <field name="arch" type="xml">
                <form string="Account Sync" >
                    <sheet>
                    <group>
                        <group>
                               <field name="host"/>
                               <field name="database"/>
                               <field name="type"/>
                        </group>
                               <field name="username"/>
                               <field name="password"/>
                        </group>
                        <group>
                               <field name="query"/>
                        </group>

                    </sheet>

                </form>
            </field>
        </record>


    	<record id="view_account_sync_connector_tree" model="ir.ui.view">
		<field name="name">account.sync.connector.tree</field>
		<field name="model">account.sync.connector</field>
		<field name="arch" type="xml">
			<tree string="Account Sync"  >
				<field name="host"/>
                <field name="type"/>
			</tree>
		</field>
	    </record>



    <record id="action_account_sync_interface" model="ir.actions.act_window">
		<field name="name">Data Interface</field>
		<field name="res_model">account.sync.connector</field>
		<field name="type">ir.actions.act_window</field>
		<field name="view_mode">tree,form</field>
		<field name="domain">[]</field>
		<field name="context">{}</field>
		<field name="help" type="html">

			<p class="o_view_nocontent_smiling_face">
				Add Config .............
			</p>

		</field>
	</record>

    <menuitem
		  id="account_sync_connector_menu"
		  name="Data Interface"
		  parent="base.menu_administration"
		  action="action_account_sync_interface"
		  sequence="32"
	/>

    </data>
</odoo>