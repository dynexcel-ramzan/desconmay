<odoo>
  <data>
           <!--Display form view of wizard-->    
        <record id="view_timesheet_report_wizard_form" model="ir.ui.view">
            <field name="name">timesheet.report.wizard.form</field>
            <field name="model">timesheet.report.wizard</field>
            <field name="arch" type="xml">
                <form string="Timesheet Report">
                    <group>
                        
                       <group>
                            <field name="start_date" />
                            <field name="type" />
                      </group>
                        
                      <group>
                           <field name="end_date" />
                      </group>                        
                        
                    </group>
                    <notebook>
                        <page string="Employees">
                            <field name="employee_ids">
                                <tree editable="bottom">
                                    <field name="id" invisible="1"/>
                                    <field name="name" />
                                    <field name="department_id" /> 
                                    <field name="company_id" />                                    
                                </tree>
                            </field>
                        </page>
                    </notebook>
                    <footer>
                        <button string='Print' name="check_report" type="object" default_focus="1" class="oe_highlight"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <!--Open the wizard on click of menu-->
        <record id="open_timesheet_report_wizard_action" model="ir.actions.act_window">
            <field name="name">Timesheet Report</field>
            <field name="res_model">timesheet.report.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="view_timesheet_report_wizard_form"/>
        </record>
      
      
        <menuitem
        id="ora_timesheet_report_menu"
        name="Timesheet Report"
        parent="hr_timesheet.menu_timesheets_reports"
        sequence="4"
        action="open_timesheet_report_wizard_action"
    />
      
     
  </data>
</odoo>